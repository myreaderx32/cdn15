            var adTypeStr = newAdtypeStr.substr(1, 1);
        } else {
            var adTypeStr = newAdtypeStr.substr(0, 1);
        }
        if (newAdflagStr.indexOf('[') > -1) {
            adFlagStr = newAdflagStr.substr(1, 1);
        } else {
            adFlagStr = newAdflagStr.substr(0, 1);
        }
